{
    "name": "ner0tic/project-mgmt-system",
    "license": "MIT",
    "type": "project",
    "description": "A CMS for developers",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ner0tic/multi-user-bundle",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/ner0tic/PUGXMultiUserBundle.git",
                    "reference": "1.4"
                },
                "target-dir": "PUGX/MultiUserBundle"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ner0tic/millwright-menu-bundle",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/ner0tic/MillwrightMenuBundle.git",
                    "reference": "master"
                },
                "require": {
                    "knplabs/knp-menu-bundle": "dev-master",
                    "ner0tic/millwright-configuration-bundle": "dev-master"
                },
                "target-dir": "Millwright/MenuBundle"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ner0tic/millwright-util",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "git://github.com/ner0tic/MillwrightUtil.git",
                    "reference": "master"
                },
                "target-dir": "Millwright/Util"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ner0tic/millwright-configuration-bundle",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "git://github.com/ner0tic/ConfigurationBundle.git",
                    "reference": "master"
                },
                "target-dir": "Millwright/ConfigurationBundle"
            }
        }
    ],
    "require": {
        "php":                                                              ">=5.4",

        "symfony/symfony":                                                  "2.2.2",
        "symfony/assetic-bundle":                                           "2.1.*",
        "symfony/swiftmailer-bundle":                                       "2.2.*",
        "symfony/monolog-bundle":                                           "2.2.*",
        "symfony/translation":                                                  "*",

        "doctrine/orm":                                              "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle":                                         "1.2.*",
        "doctrine/dbal":                                                  "2.2.2.0",
        "doctrine/doctrine-fixtures-bundle":                           "dev-master",
        "doctrine/data-fixtures":                                       "1.0.x-dev",

        "twig/extensions":                                                  "1.0.*",

        "sensio/distribution-bundle":                                       "2.2.*",
        "sensio/framework-extra-bundle":                                    "2.2.*",
        "sensio/generator-bundle":                                          "2.2.*",

        "jms/security-extra-bundle":                                        "1.4.*",
        "jms/di-extra-bundle":                                              "1.3.*",

        "gedmo/doctrine-extensions":                                    "2.3.x-dev",
        "stof/doctrine-extensions-bundle":                               "~1.1@dev",

        "friendsofsymfony/user-bundle":                                     "1.3.1",
        "ner0tic/multi-user-bundle":                                   "dev-master",

        "white-october/pagerfanta-bundle":                             "dev-master",
        "pagerfanta/pagerfanta":                                       "dev-master",

        "braincrafted/bootstrap-bundle":                                    "1.4.2",
        "twitter/bootstrap":                                                "2.3.*",
        "leafo/lessphp":                                               "dev-master",

        "knplabs/knp-menu":                                             "2.0.x-dev",
        "knplabs/knp-menu-bundle":                                      "dev-master",

        "ner0tic/millwright-util":                                      "dev-master",
        "ner0tic/millwright-configuration-bundle":                      "dev-master",
        "ner0tic/millwright-menu-bundle":                               "dev-master"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle":                             "dev-master",
        "jms/debugging-bundle":                                       "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "alpha",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "PMS": "src/",
            "PUGX\\MultiUserBundle": "vendor/ner0tic/multi-user-bundle/",
            "Millwright\\ConfigurationBundle": "vendor/ner0tic/millwright-configuration-bundle/",
            "Millwright\\Util": "vendor/ner0tic/millwright-util/",
            "Millwright\\MenuBundle": "vendor/ner0tic/millwright-menu-bundle/"
        }
    }
}
